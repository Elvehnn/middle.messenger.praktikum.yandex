{{!< desktop}}

<main class="main">
    <form class="login-form" action="./main.html">
    <h3>Sign up</h3>
        <ul class="login-form__group">
            {{#each signup-inputs}}
                <li>
                {{> 'textInput/textInput' type=type text=text}}
                </li>
            {{/each}}
        </ul>
 
       
        <div class="login-form__group">
            <a class="login-form__link" href="./main.hbs">{{> 'button/button' button-title='Sign up' btn-class='button button_confirm'}}</a>
            <a class="login-form__link" href="./index.hbs">Sign in</a>
        </div>
       
        
    </form>
   
</main>