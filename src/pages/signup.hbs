{{!< desktop}}

<main class="main">
    <form class="login-form" action="./main.html">
        <div class="login-form__group">
           {{> 'textInput/textInput' input-type=login-input.type input-text=login-input.text}}
            {{> 'textInput/textInput' input-type=password-input.type input-text=password-input.text}}
        </div>
       
        <div class="login-form__group">
            <a class="login-form__link" href="./main.hbs">{{> 'button/button' button-title='Sign up' btn-class='button button_confirm'}}</a>
            <a class="login-form__link" href="./main.hbs">{{> 'button/button' button-title='Sign in' btn-class='button button_redirect'}}</a>
        </div>
       
        
    </form>
   
</main>